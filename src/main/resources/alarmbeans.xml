<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="doorBeanWithSoundAlarm" class="com.subbu.subbupamapna.demo.springconfigsalarm.Door">
    
      <property name="alarm" ref="soundAlarmBean"></property>
    </bean>
    
     <bean id="doorBeanWithVisualAlarm" class="com.subbu.subbupamapna.demo.springconfigsalarm.Door">
    
      <property name="alarm" ref="visualAlarmBean"></property>
    </bean>
    
     <bean id="soundAlarmBean" class="com.subbu.subbupamapna.demo.springconfigsalarm.SoundAlarm">
    
      
    </bean>
    
     <bean id="visualAlarmBean" class="com.subbu.subbupamapna.demo.springconfigsalarm.VisualAlarm">
    
      
    </bean>
    
    

</beans>